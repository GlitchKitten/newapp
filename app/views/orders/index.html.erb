<h2> Order Index Page</h2>
<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>Product ID</th>
      <th>Total</th>
      <th>Actions</th>
    </tr> 
   <thead> 
  
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.user_id %></td>
        <td><%= order.product_id %></td>
        <td><%= order.total %></td>
        <td><%= link_to 'Show', order %></td>
      </tr>
    <% end %>
  </tbody>
  
</table>